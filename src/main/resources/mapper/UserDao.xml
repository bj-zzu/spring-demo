<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="cn.zzuzl.dao.UserDao">
    <insert id="addUser" parameterType="cn.zzuzl.domain.User">
        insert into user(
        username,
        password
        ) values(
        #{username},
        #{password}
        )
    </insert>
</mapper>